bindkey -k k4 eval 'next' 'fit'				# F4  | Next Window

# Make it more logical how to enter scrollback
bindkey "^[[5;3~" copy					# alt-pageup | scrollback
bindkey "^[[6;3~" copy					# alt-pageup | scrollback
