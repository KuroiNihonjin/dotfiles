#! /bin/sh

./configure \
--with-features=normal \
--prefix=/home/thib/data/bin/vim \
--enable-rubyinterp \
--enable-multibyte \
--enable-cscope \
--disable-netbeans \
--disable-darwin \
--disable-selinux \
--disable-workshop \
--disable-gui \
--disable-gtk2-check \
--disable-gnome-check \
--disable-gtktest \
--without-gnome
